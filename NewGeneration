local HighestLevelToFarm = 0
local questmodule = require(game.ReplicatedStorage.Quests)
local AutoUpdate

function GetLevel()
    pcall(function()
        Lv = game.Players.LocalPlayer.Data.Level.Value
    if Sea3 then
    if Lv > LevelMax then
        AutoUpdate = true
  Lv = game.Players.LocalPlayer.Data.Level.Value
for i,v in pairs(questmodule) do
    for i2,v2 in pairs(v) do
        for i3,v3 in pairs(v2) do
            for i6,v6 in pairs(v2) do
            if i6 == "Task" then
                for i7,v7 in pairs(v6) do
                    if v7 ~= 1 and v7 ~= 50 then
                if typeof(v3) == "number" then
                if v3 <= Lv then
                    local Number = tonumber(v3)
                    if Number > HighestLevelToFarm then
                        HighestLevelToFarm = Number
                        wait()
                        if v2.LevelReq <= HighestLevelToFarm then
                        CurrentQuest = i
                        CurrentQuestLv = i2
                        MobName = v2.Name
                    end
end
end
end
end
end
end
end
end
end
end
local NearestQuestsNPC = math.huge
for i,v in pairs(game:GetService("Workspace").NPCs:GetChildren()) do
    for x,y in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							if y:FindFirstChild("Humanoid") and y.Humanoid.Health > 0 then
    if v:FindFirstChild('Head') then
                  if v.Head:FindFirstChild("Talk") and v.Head:FindFirstChild("QuestBBG") and v.Head:FindFirstChild("QuestBBG").Title.Text == "QUEST" then
                        if (v.HumanoidRootPart.Position-y.HumanoidRootPart.Position).magnitude < NearestQuestsNPC then
                            NearestQuestsNPC = (v.HumanoidRootPart.Position-y.HumanoidRootPart.Position).magnitude
                            CFrameQuest = v
end
end
end
end
end
end
