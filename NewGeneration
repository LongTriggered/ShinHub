GetLevel()

local combat = getupvalues(require(game.Players.LocalPlayer.PlayerScripts.CombatFramework))
local Rig = combat[2]

spawn(function()
	while wait() do
		if set.AutoFarmLevel and Safe() then
		    wait()
		    pcall(function()  
		      --  if Safe() then
		            GetLevel()
		            if AutoUpdate then
		            for i, v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
							        if string.find(v.Name, MobName) then
							            v.Parent = game:GetService("Workspace").Enemies
							        end
		            end
						        if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
						            FastAttack = false
						            for i2,v2 in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
						            if #game:GetService("Workspace").NPCs:GetChildren() > 0 then
						               for i,v in pairs(game:GetService("Workspace").NPCs:GetChildren()) do
                                          if v:FindFirstChild("Head") and v.Head:FindFirstChild("Talk") and v.Head:FindFirstChild("QuestBBG") then
						                    TP(CFrameQuest.HumanoidRootPart.CFrame)
						                    else
						                        if string.find(v2.Name, MobName) and v2:FindFirstChild("Humanoid") and v2.Humanoid.Health > 0 then
							           game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2048,2.0199999809265137,2048)
							    TP(v2.HumanoidRootPart.CFrame  * CFrame.new(0,10,0))
                                             end
                                          end
				                    end
						            else
					                if string.find(v2.Name, MobName) and v2:FindFirstChild("Humanoid") and v2.Humanoid.Health > 0 then
							           game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2048,2.0199999809265137,2048)
							    TP(v2.HumanoidRootPart.CFrame  * CFrame.new(0,10,0))
							    end
							end
							    end
				    TP(CFrameQuest.HumanoidRootPart.CFrame)
				if (CFrameQuest.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 4 then
					wait(0.7)
						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", CurrentQuest, CurrentQuestLv)
						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
				end
			elseif game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
						for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							if string.find(v.Name, MobName) and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
									repeat game:GetService("RunService").Heartbeat:wait()
									    if string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, MobName) then
												EquipWeapon(set.PickedWeapon)
                                                BringMob()
                                                spawn(function()
                                                Rig.activeController.timeToNextAttack = 0
												Rig.activeController.hitSound = nil
                                                Rig.activeController.hitboxMagnitude = 60
                                                Rig.activeController:attack()    
                                                end)
                                                game:GetService("VirtualUser"):CaptureController()
												game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 670),workspace.CurrentCamera.CFrame)
												FastAttack = true
												TP(v.HumanoidRootPart.CFrame * CFrame.new(0,set.Y,0))
                                                PosMon = v.HumanoidRootPart.CFrame
												game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(2,2.0199999809265137,1)
												v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
											     v.HumanoidRootPart.CanCollide = false
												else
												game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
											end
									until not v.Parent or v.Humanoid.Health <= 0 or set.AutoFarmLevel == false or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false or not game:GetService("Workspace").Enemies:FindFirstChild(v.Name)
							else
								    FastAttack = false
							end
						end
			end
		            elseif not AutoUpdate then
				  if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
				      FastAttack = false
				      TP(CFrameQuest)
				      if (CFrameQuest.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 4 then
					wait(0.7)
						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", CurrentQuest, CurrentQuestLv)
						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
						else
						TP(CFrameQuest)
				end
				  elseif game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
				      if game:GetService("Workspace").Enemies:FindFirstChild(MobFarmName) then
		         for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							if v.Name == MobFarmName and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
									repeat game:GetService("RunService").Heartbeat:wait()
									    if string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, MobName) then
												EquipWeapon(set.PickedWeapon)
												BringMob()
                                                spawn(function()
                                                    Rig.activeController.timeToNextAttack = 0
                                                    Rig.activeController.hitSound = nil
                                                    Rig.activeController.hitboxMagnitude = 60
                                                    Rig.activeController:attack() 
                                                end)
                                                game:GetService("VirtualUser"):CaptureController()
												game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 670),workspace.CurrentCamera.CFrame)
												TP(v.HumanoidRootPart.CFrame * CFrame.new(0,set.Y,0))
												PosMon = v.HumanoidRootPart.CFrame
												v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
											     v.HumanoidRootPart.CanCollide = false
												else
												game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
											end
									until not v.Parent or v.Humanoid.Health <= 0 or set.AutoFarmLevel == false or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false or not game:GetService("Workspace").Enemies:FindFirstChild(v.Name)
							end
		         end
		         else
		             TP(FarmCFrame)
					end
			end
			end
		
		    end)
		    else
		        FastAttack = false
end
end
end)

spawn(function()
while wait(1) do
    if set.AutoStatMelee then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", "Melee", SelectPoint)
       -- UpdateMelee:UpdateToggle("Current Melee: "..Melee)
    end
end
end)
spawn(function()
    while wait(1) do
    if set.AutoStatDefense then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", "Defense", SelectPoint)
       -- UpdateDefense:UpdateToggle("Current Defense: "..Defense)
    end
end
end)
spawn(function()
    while wait(1) do
    if set.AutoStatSword then
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", "Sword", SelectPoint)
    -- UpdateSword:UpdateToggle("Current Sword: "..Sword)
    end
end
end)
spawn(function()
    while wait(1) do
    if set.AutoStatGun then
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", "Gun", SelectPoint)
   -- UpdateGun:UpdateToggle("Current Gun: "..Gun)
    end
end
    end)
    spawn(function()
        while wait(1) do
if set.AutoStatBloxFruit then
game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", "Demon Fruit", SelectPoint)
--UpdateDemonFruit:UpdateToggle("Current Blox Fruit: "..DemonFruit)
    end
end
end)

function BringMob()
    pcall(function()
        for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
            if set.AutoFarmLevel and not AutoUpdate then
                if v.Name == MobFarmName and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 320 then
                            v.HumanoidRootPart.CFrame = PosMon
                            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif set.AutoFarmLevel and AutoUpdate then
                if string.find(v.Name, MobName) and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 320 then
                            v.HumanoidRootPart.CFrame = PosMon
                            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif FarmMasteryFruit and MasteryMagnet then
                    if not string.find(v.Name, "Boss") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CFrame = PosMon
                            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                    end
            elseif FarmMasteryGun and MasteryMagnet then
                    if not string.find(v.Name, "Boss") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CFrame = PosMon
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                            v.HumanoidRootPart.CanCollide = false
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                    end
            elseif AutoBartilo and set.Magnet then
                if v.Name == "Swan Pirate [Lv. 775]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoRengoku and set.Magnet then
                if (v.Name == "Snow Lurker [Lv. 1375]" or v.Name == "Arctic Warrior [Lv. 1350]") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            
            elseif Auto_Bone and set.Magnet then
                if (v.Name == "Reborn Skeleton [Lv. 1975]" or v.Name == "Living Zombie [Lv. 2000]" or v.Name == "Demonic Soul [Lv. 2025]" or v.Name == "Posessed Mummy [Lv. 2050]") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoEcto and set.Magnet then
                if string.find(v.Name, "Ship") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoEvoRace and set.Magnet then
                if v.Name == "Zombie [Lv. 950]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoCitizen and set.Magnet then
                if v.Name == "Forest Pirate [Lv. 1825]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
                    elseif AutoCandy and CandyMagnet and set.Magnet then
                if v.Name == "Head Baker [Lv. 2275]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
                        elseif AutoCandy and CandyMagnet2 and set.Magnet then
                if v.Name == "Cookie Crafter [Lv. 2200]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
                        elseif AutoCandy and CandyMagnet3 and set.Magnet then
                if v.Name == "Cake Guard [Lv. 2225]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
                        elseif AutoCandy and CandyMagnet4 and set.Magnet then
                if v.Name == "Baking Staff [Lv. 2250]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                            end
        end
        end
end)
end
