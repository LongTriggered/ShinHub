--Library--
local Window = Library.CreateLib("SHIN HUB ðŸ”¥ðŸ”¥ðŸ”¥", "GrapeTheme")
local Tab = Window:NewTab("Auto Farm")
local Section = Tab:NewSection("Auto Farm")
WeaponSection = Section:NewDropdown("Select Weapons","", Weapons, function(Value)
  set.PickedWeapon = Value
  end)
  Section:NewButton("Refresh Weapons", "Refreshes Dropdown", function()
    for k in pairs (Weapons) do
        Weapons [k] = nil
    end
    for i,v in pairs(game:GetService('Players').LocalPlayer.Backpack:GetChildren()) do  
        if v:IsA("Tool") then
           table.insert(Weapons ,v.Name)
        end
    end
    for i,v in pairs(game:GetService('Players').LocalPlayer.Character:GetChildren()) do  
        if v:IsA("Tool") then
           table.insert(Weapons, v.Name)
        end
    end
    dropdown:Refresh(Weapons)
  end)

  if set.PickedWeapon ~= nil then
    WeaponSection:UpdateDrop(set.PickedWeapon)
end


  Section:NewToggle("Auto Farm Level", "ToggleInfo", set.AutoFarmLevel, function(state)
    if state and set.PickedWeapon == nil then
        Notification.Notify(" Auto Farm ", "Please Select Weapon", "rbxassetid://2484550853", {
            Duration = 3,
            
            TitleSettings = {
                TextXAlignment = Enum.TextXAlignment.Center,
                Font = Enum.Font.SourceSansSemibold,
            },
        
            GradientSettings = {
                GradientEnabled = false,
                SolidColorEnabled = true,
                SolidColor = Color3.fromRGB(124, 83, 240),
                Retract = true
            }
        })
    else
        HighestLevelToFarm = 0
        set.AutoFarmLevel = state
        if state == false then
            TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
            game.Players.LocalPlayer.Character.UpperTorso:FindFirstChild('BodyVelocity'):Destroy()
        end
    end
end)
  if Sea3 then
Section:NewToggle("Auto get Electric Claw", "ToggleInfo", function(state)
        if state then
            print(SelectWeapon)
        else
            print("Toggle Off")
        end
    end)
Section:NewToggle("Auto get Rainbow Haki", "ToggleInfo", function(state)
    if state then
        print(SelectWeapon)
    else
        print("Toggle Off")
    end
end)
Section:NewToggle("Auto get Electric Claw", "ToggleInfo", function(state)
    if state then
        print(SelectWeapon)
    else
        print("Toggle Off")
    end
end)
Section:NewToggle("Auto Citizen Quest", "ToggleInfo", function(state)
    if state then
        print(SelectWeapon)
    else
        print("Toggle Off")
    end
end)
Section:NewToggle("Auto Farm Elite Hunter", "ToggleInfo", function(state)
    if state then
        print(SelectWeapon)
    else
        print("Toggle Off")
    end
end)
end
