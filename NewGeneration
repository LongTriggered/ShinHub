local LevelMax = 2400
local SaveSettingName = "ShinHubData.txt"
    local DefaultSettings = {
        Marine = true,
        Pirate = false,
        PickedWeapon,
        AutoFarmLevel = false,
        Y = 30,
        AutoStatMelee = false,
        AutoStatDefense = false,
        AutoStatSword = false,
        AutoStatGun = false,
        AutoStatBloxFruit = false,
        AutoBusoHaki = true,
        AutoObservationHaki = false,
        AutoRandomFruit = false,
        LawPickedWeapons,
        KatakuriPickedWeapons,
        StopTP = true,
        AutoSave = true,
        AutoGetQuest = true,
        FastAttack = true
        }
        local set
        
        if not pcall(function() readfile(SaveSettingName) end) then writefile(SaveSettingName, game:service'HttpService':JSONEncode(DefaultSettings)) end
        
        set = game:service'HttpService':JSONDecode(readfile(SaveSettingName))
        
        local function click(x, y, button)
            button -= 1
            game:GetService("VirtualInputManager"):SendMouseButtonEvent(x, y, button, true, game, 0)
            game:GetService("VirtualInputManager"):SendMouseButtonEvent(x, y, button, false, game, 0)
          end

        repeat wait()
            if game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("Main"):FindFirstChild('ChooseTeam') then
                if set.Pirate then
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Pirates.Frame.ViewportFrame.TextButton.Size = UDim2.new(1000, 10000, 1000, 10000)
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Pirates.Frame.ViewportFrame.TextButton.Position = UDim2.new(-4, 0, -5, 0)
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Pirates.Frame.ViewportFrame.TextButton.BackgroundTransparency = 1
                    wait(.5)
                    
                    click(200, 500, 1) -- Clicks pixel 200, 500 with mouse button 1
                elseif set.Marine then
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Marines.Frame.ViewportFrame.TextButton.Size = UDim2.new(1000, 10000, 1000, 10000)
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Marines.Frame.ViewportFrame.TextButton.Position = UDim2.new(-4, 0, -5, 0)
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Marines.Frame.ViewportFrame.TextButton.BackgroundTransparency = 1
                    wait(.5)
                   click(200, 500, 1)
                else
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Marines.Frame.ViewportFrame.TextButton.Size = UDim2.new(1000, 10000, 1000, 10000)
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Marines.Frame.ViewportFrame.TextButton.Position = UDim2.new(-4, 0, -5, 0)
                    game:GetService("Players")["LocalPlayer"].PlayerGui.Main.ChooseTeam.Container.Marines.Frame.ViewportFrame.TextButton.BackgroundTransparency = 1
                    wait(.5)
                    click(200, 500, 1)
                end
            end
        until game.Players.LocalPlayer.Team ~= nil and game:IsLoaded() and not game:GetService("Players").LocalPlayer.PlayerGui.Main:FindFirstChild('ChooseTeam') 

local hackxin = false
if setfflag and not KRNL_LOADED then
    setfflag("HumanoidParallelRemoveNoPhysics", "False")
setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
hackxin = true
end
print('hackxin = ', hackxin)
require(game.ReplicatedStorage.Util.CameraShaker):Stop()
require(game.Players.LocalPlayer.PlayerScripts.CombatFramework.Particle).play=function(...)end
if game.ReplicatedStorage.Effect.Container:FindFirstChild('Death') then
game.ReplicatedStorage.Effect.Container:FindFirstChild('Death'):Destroy()
end

local CharParts = {}
game:GetService("RunService").Stepped:connect(function()
    if hackxin and not NoclipMisc then
    if Clip or set.AutoFarmLevel or AutoFarmBones or AutoFarmKatakuri or AutoFarmLaw or NextIsland or TeleportToPlayer or AutoRaid or AutoFarmSelectedMob then
        game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
    end
elseif not hackxin and not NoclipMisc then
    if Clip or set.AutoFarmLevel or AutoFarmBones or AutoFarmKatakuri or AutoFarmLaw or NextIsland or TeleportToPlayer or AutoRaid or AutoFarmSelectedMob then
        for i = 1, #CharParts do
            CharParts[i].CanCollide = false
        end
    end
elseif hackxin or not hackxin and NoclipMisc then
    for i = 1, #CharParts do
        CharParts[i].CanCollide = false
    end
    end
end)

spawn(function()
    while wait() do
        if not hackxin then
         if Clip or set.AutoFarmLevel or AutoFarmBones or AutoFarmKatakuri or AutoFarmLaw or NextIsland or TeleportToPlayer or AutoRaid or AutoFarmSelectedMob then
             pcall(function()
            if not game.Players.LocalPlayer.Character.UpperTorso:FindFirstChild('BodyVelocity') then
        local BV = Instance.new('BodyVelocity')
         BV.Parent = game.Players.LocalPlayer.Character.UpperTorso
         BV.velocity = Vector3.new(0, 0, 0)
         BV.maxForce = Vector3.new(9e9, 9e9, 9e9)
         end
             end)
     end
         end
        end
 end)

function SetupCharCollide(Char)
    CharParts = {}
    Char:WaitForChild("Head")
    for i, v in pairs(Char:GetChildren()) do
        if v:IsA("BasePart") then
            table.insert(CharParts, v)
        end
    end
end

if game.Players.LocalPlayer.Character then
    SetupCharCollide(game.Players.LocalPlayer.Character)
end
game.Players.LocalPlayer.CharacterAdded:connect(function(Ch)
    if not hackxin then
    SetupCharCollide(Ch)
    end
end)

local Plr = game:GetService("Players").LocalPlayer
pcall(function()
    local Mouse = Plr:GetMouse()
    Mouse.Button1Down:connect(function()
        if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then
            return
        end
        if not Mouse.Target then
            return
        end
        if ClickTp then
            Plr.Character:MoveTo(Mouse.Hit.p)
        end
    end)
    end)

    function EquipWeapon(ToolSe)
        if Safe() then
        if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
            local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
            wait(.4)
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
        end
        end
    end

    local atkplay = game.Players.LocalPlayer

    local CbFw = debug.getupvalues(require(atkplay.PlayerScripts.CombatFramework))
    local CbFw2 = CbFw[2]
    
    function GetBlade() 
        local p13 = CbFw2.activeController
        local ret = p13.blades[1]
        if not ret then return end
        while ret.Parent~=game.Players.LocalPlayer.Character do ret=ret.Parent end
        return ret
    end
    function Attack(time) 
        pcall(function()
        local AC = CbFw2.activeController
        for i = 1, 1 do 
            local bladehit = require(game.ReplicatedStorage.CombatFramework.RigLib).getBladeHits(
                atkplay.Character,
                {atkplay.Character.HumanoidRootPart},
                60
            )
            local cac = {}
            local hash = {}
            for k, v in pairs(bladehit) do
                if v.Parent:FindFirstChild("HumanoidRootPart") and not hash[v.Parent] then
                    table.insert(cac, v.Parent.HumanoidRootPart)
                    hash[v.Parent] = true
                end
            end
            bladehit = cac
            if #bladehit > 0 then
                local u8 = debug.getupvalue(AC.attack, 5)
                local u9 = debug.getupvalue(AC.attack, 6)
                local u7 = debug.getupvalue(AC.attack, 4)
                local u10 = debug.getupvalue(AC.attack, 7)
                local u12 = (u8 * 798405 + u7 * 727595) % u9
                local u13 = u7 * 798405
                (function()
                    u12 = (u12 * u9 + u13) % 1099511627776
                    u8 = math.floor(u12 / u9)
                    u7 = u12 - u8 * u9
                end)()
                u10 = u10 + 1
                debug.setupvalue(AC.attack, 5, u8)
                debug.setupvalue(AC.attack, 6, u9)
                debug.setupvalue(AC.attack, 4, u7)
                debug.setupvalue(AC.attack, 7, u10)
                pcall(function()
                    for k, v in pairs(AC.animator.anims.basic) do
                        v:Play()
                    end                  
                end)
                if atkplay.Character:FindFirstChildOfClass("Tool") and AC.blades and AC.blades[1] then 
                    game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("weaponChange",tostring(GetBlade()))
                    game.ReplicatedStorage.Remotes.Validator:FireServer(math.floor(u12 / 1099511627776 * 16777215), u10)
                    game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("hit", bladehit, i, "") 
                    wait(time)
                end
            end
        end
    end)
    end


function Safe()
    if game:GetService("Players").LocalPlayer and game:GetService("Players").LocalPlayer.Character then
									        if game:GetService("Players").LocalPlayer.Character:FindFirstChild('Humanoid') and game:GetService("Players").LocalPlayer.Character:FindFirstChild('HumanoidRootPart') then
									            if game:GetService("Players").LocalPlayer.Character:FindFirstChild('Humanoid').Health > 0 then
									                return true
									            end
									        end
                                       end
    return false
						                end

function TP(P1)
 pcall(function()
if Safe() then
Distance = (P1.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
if Distance <= 300 then
Speed = 2e9
-- elseif Distance < 500 and Distance > 250 then
-- Speed = 400
elseif Distance < 1000 then
Speed = 325
 elseif Distance >= 1000 then
 Speed = 300
end
tping = game:GetService("TweenService"):Create(
 game.Players.LocalPlayer.Character.HumanoidRootPart,
TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
{CFrame = P1 * CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(0))})
tping:Play()
end
end)
 end

 spawn(function()
    local LastPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
    while task.wait() do
        if tping then
            if (LastPos-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 400 then
                tping:Cancel()
                wait(0.5)
            end
           LastPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        end
    end
end)

function TP2(P1)
    pcall(function()
        if Safe() then
            LastPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
	Distance = (P1.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
	if Distance < 1000 then
		Speed = 400
	elseif Distance >= 1000 then
		Speed = 250
	end
    tpingz = game:GetService("TweenService"):Create(
        game.Players.LocalPlayer.Character.HumanoidRootPart,
        TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
        {CFrame = P1 * CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(0))}
    )
    tpingz:Play()
    Clip = true
    tpingz.Completed:wait()
    Clip = false
    game.Players.LocalPlayer.Character.UpperTorso:FindFirstChild('BodyVelocity'):Destroy()
end
end)
end
