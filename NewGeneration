                                wait(3)
                                 if v.Humanoid.Health == v.Humanoid.MaxHealth then
             v.Humanoid.Health = 0
                                 end
                             end)
                            end
                            until v.Humanoid.Health <= 0 or not v.Parent or not AutoFarmKatakuri   
                      end
                      end
                    end
                end
                    else
                        FastAttackBoss = false
                        TP(CFrame.new(-2139.533203125, 60.00886535644531, -12308.591796875))     
                  end
                end
end)
          end
         end
end)



                                  --  if v.Name == "Reborn Skeleton [Lv. 1975]" or v.Name == "Living Zombie [Lv. 2000]" or v.Name == "Demonic Soul [Lv. 2025]" or v.Name == "Posessed Mummy [Lv. 2050]" then
BringBone1 = false
BringBone2 = false
