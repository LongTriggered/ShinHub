spawn(function()
		while wait() do
			if AutoFarmLaw then
                pcall(function()
			        if Safe() then
                        spawn(function()
                            if not game:GetService("Workspace").Enemies:FindFirstChild("Order [Lv. 1250] [Raid Boss]") and not game:GetService("ReplicatedStorage"):FindFirstChild("Order [Lv. 1250] [Raid Boss]") and game:GetService("Players").LocalPlayer.Character:FindFirstChild("Microchip") or game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Microchip") then
                                fireclickdetector(game:GetService("Workspace").Map.CircleIsland.RaidSummon.Button.Main.ClickDetector)
                            end
                        end)
					if game:GetService("Workspace").Enemies:FindFirstChild("Order [Lv. 1250] [Raid Boss]") then
						for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							if v.Name == "Order [Lv. 1250] [Raid Boss]" and v.Humanoid.Health > 0 then
								repeat game:GetService("RunService").Heartbeat:wait()
									EquipWeapon(set.LawPickedWeapons)
                                    spawn(function()
                                        pcall(function()
                                        getupvalues(require(game.Players.LocalPlayer.PlayerScripts.CombatFramework))[2].activeController.timeToNextAttack = 0                                                    
                                        getupvalues(require(game.Players.LocalPlayer.PlayerScripts.CombatFramework))[2].activeController.hitboxMagnitude = 60
                                        getupvalues(require(game.Players.LocalPlayer.PlayerScripts.CombatFramework))[2].activeController:attack() 
                                        end)
                                    end)
									TP(v.HumanoidRootPart.CFrame * CFrame.new(0,50,30))
									v.HumanoidRootPart.CanCollide = false
                                    v.Humanoid.PlatformStand = true
                                    sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
									game:GetService("VirtualUser"):CaptureController()
									game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 670),workspace.CurrentCamera.CFrame)
								until not v.Parent or v.Humanoid.Health == 0 or AutoFarmLaw == false
                            else
                                TP(CFrame.new(-6217.2021484375, 28.047645568848, -5053.1357421875))
							end
						end
					else
						TP(CFrame.new(-6217.2021484375, 28.047645568848, -5053.1357421875))
					end
				end
			        end)
			end
	end
end)

local Tab = Window:NewTab("Devil Fruit")
local Section = Tab:NewSection("Devil Fruit")
SelectDF = ""
Section:NewDropdown2("Select Devil Fruit", "DropdownInf", FruitTable, function(currentOption)
    SelectDF = currentOption
    print(unpack(currentOption))
end)

spawn(function()
    while wait() do
        if BuyFruitSinper then
            if game.Players.LocalPlayer.Data.DevilFruit.Value ~= unpack(SelectDF) then
            if #SelectDF == 1 then
                local args = {
                    [1] = "GetFruits"
                }
                
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                local args = {
                    [1] = "PurchaseRawFruit",
                    [2] = SelectDF[1]
                }
                
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            elseif #SelectDF > 1 and #SelectDF < 3 then
            local args = {
                [1] = "GetFruits"
            }
            
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            local args = {
                [1] = "PurchaseRawFruit",
                [2] = SelectDF[1]
            }
            
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            local args = {
                [1] = "PurchaseRawFruit",
                [2] = SelectDF[2]
            }
            
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        elseif #SelectDF > 2 and #SelectDF < 4 then
        local args = {
            [1] = "GetFruits"
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = SelectDF[1]
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = SelectDF[2]
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = SelectDF[3]
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    elseif #SelectDF > 3 and #SelectDF < 5 then
        local args = {
            [1] = "GetFruits"
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = SelectDF[1]
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = SelectDF[2]
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = SelectDF[3]
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = SelectDF[4]
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    elseif #SelectDF > 4 then
        local args = {
            [1] = "GetFruits"
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = unpack(SelectDF)
        }
        
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    end
        end 
    end
    end
end)

Section:NewToggle("Auto Buy Selected Devil Fruit", "ToggleInfo",BuyFruitSinper, function(state)
    BuyFruitSinper = state
end)
Section:Line()

Section:NewButton("Random Devil Fruit", "ButtonInfo", function()
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Cousin","Buy")
end)

Section:NewToggle("Auto Random Devil Fruit", "ToggleInfo",set.AutoRandomFruit, function(state)
    set.AutoRandomFruit = state
end)
Section:NewToggle("Auto Store Devil Fruit", "ToggleInfo",AutoStoreFruit , function(state)
    AutoStoreFruit  = state
end)
spawn(function()
    while wait() do
        if set.AutoRandomFruit then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Cousin","Buy")
        end
    end
end)

