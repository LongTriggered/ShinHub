    sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  9e20)
end)
end)

function BringMob()
    pcall(function()
        for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
            if set.AutoFarmLevel or AutoFarmSelectedMob and not AutoUpdate then
                if v.Name == MobFarmName and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CFrame = PosMon
                            v.HumanoidRootPart.CanCollide = false
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif set.AutoFarmLevel and AutoUpdate then
                if string.find(v.Name, MobName) and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CFrame = PosMon
                            v.HumanoidRootPart.CanCollide = false
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif FarmMasteryFruit and MasteryMagnet then
                    if not string.find(v.Name, "Boss") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CFrame = PosMon
                            v.HumanoidRootPart.CanCollide = false
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                    end
            elseif FarmMasteryGun and MasteryMagnet then
                    if not string.find(v.Name, "Boss") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CFrame = PosMon
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.CanCollide = false
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                    end
            elseif AutoBartilo and set.Magnet then
                if v.Name == "Swan Pirate [Lv. 775]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoRengoku and set.Magnet then
                if (v.Name == "Snow Lurker [Lv. 1375]" or v.Name == "Arctic Warrior [Lv. 1350]") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            
            elseif Auto_Bone and set.Magnet then
                if (v.Name == "Reborn Skeleton [Lv. 1975]" or v.Name == "Living Zombie [Lv. 2000]" or v.Name == "Demonic Soul [Lv. 2025]" or v.Name == "Posessed Mummy [Lv. 2050]") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoEcto and set.Magnet then
                if string.find(v.Name, "Ship") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                       
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoEvoRace and set.Magnet then
                if v.Name == "Zombie [Lv. 950]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                      
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoCitizen and set.Magnet then
                if v.Name == "Forest Pirate [Lv. 1825]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 325 then
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.PlatformStand = true
                         
                            v.HumanoidRootPart.CFrame = PosMon
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                end
            elseif AutoFarmKatakuri and BringKata1 then
                if v.Name == "Head Baker [Lv. 2275]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 then
                    v.HumanoidRootPart.CanCollide = false    
                    v.Humanoid.PlatformStand = true  
                    v.HumanoidRootPart.CFrame = PosMon1
                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                end
                        elseif AutoFarmKatakuri and BringKata2 then
                if v.Name == "Cookie Crafter [Lv. 2200]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 then
                    v.HumanoidRootPart.CanCollide = false    
                    v.Humanoid.PlatformStand = true  
                    v.HumanoidRootPart.CFrame = PosMon2
                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                end
                        elseif AutoFarmKatakuri and BringKata3 then
                if v.Name == "Cake Guard [Lv. 2225]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 then
                    v.HumanoidRootPart.CanCollide = false    
                    v.Humanoid.PlatformStand = true  
                    v.HumanoidRootPart.CFrame = PosMon3
                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                end
                        elseif AutoFarmKatakuri and BringKata4 then
                if v.Name == "Baking Staff [Lv. 2250]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 then
                    v.HumanoidRootPart.CanCollide = false    
                    v.Humanoid.PlatformStand = true  
                    v.HumanoidRootPart.CFrame = PosMon4
                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            end
                        elseif AutoFarmBones and BringBone1 then
                if v.Name == "Posessed Mummy [Lv. 2050]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 then
                    v.HumanoidRootPart.CanCollide = false       
                    v.Humanoid.PlatformStand = true        
                    v.HumanoidRootPart.CFrame = PosMon
                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            end
                        elseif AutoFarmBones and BringBone2 then
                if v.Name == "Living Zombie [Lv. 2000]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 then
                    v.HumanoidRootPart.CanCollide = false       
                    v.Humanoid.PlatformStand = true        
                    v.HumanoidRootPart.CFrame = PosMon1
                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            end
                        elseif AutoFarmBones and BringBone3 then
                if v.Name == "Demonic Soul [Lv. 2025]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 then
                    v.HumanoidRootPart.CanCollide = false       
                    v.Humanoid.PlatformStand = true        
                    v.HumanoidRootPart.CFrame = PosMon
                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                            end
                            elseif AutoFarmBones and BringBone4 then
                if v.Name == "Reborn Skeleton [Lv. 1975]" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 300 and (v.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 300 then
                    v.HumanoidRootPart.CanCollide = false       
                    v.Humanoid.PlatformStand = true        
                    v.HumanoidRootPart.CFrame = PosMon2
                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                                        end
        end
        end
end)
end

spawn(function()
    while wait() do
        if set.AutoBusoHaki then
            if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
        end
    end
    end
