                                    Attack(task.wait(0.05))  
                              spawn(function()
                                wait(3)
                                 if v.Humanoid.Health == v.Humanoid.MaxHealth then
             v.Humanoid.Health = 0
                                 end
                             end)
                            end
                            until v.Humanoid.Health <= 0 or not v.Parent or not AutoFarmKatakuri
                      end
                      end
                    elseif c.Name == "Cake Guard [Lv. 2225]" and c.Humanoid.Health > 0 then
                            BringKata1 = false
                            BringKata2 = false
                            BringKata3 = true
                            BringKata4 = false
                            FastAttackBoss = false
                      for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                      if v.Name == "Cake Guard [Lv. 2225]" then
                          repeat game:GetService("RunService").Heartbeat:wait()
                            if v.Name == v.Name then
                                spawn(function()
                                BringMob()
                                end)
                                spawn(function()
                                PosMon3 = v.HumanoidRootPart.CFrame  
                                end)
                                    EquipWeapon(set.KatakuriPickedWeapons)
                                    TP(v.HumanoidRootPart.CFrame * CFrame.new(0,set.Y,0))         
                                    Attack(task.wait(0.05))  
                              spawn(function()
                                wait(3)
                                 if v.Humanoid.Health == v.Humanoid.MaxHealth then
             v.Humanoid.Health = 0
                                 end
                             end)
                            end
                            until v.Humanoid.Health <= 0 or not v.Parent or not AutoFarmKatakuri
                      end
                      end
                    elseif c.Name == "Baking Staff [Lv. 2250]" and c.Humanoid.Health > 0 then
                            BringKata1 = false
                            BringKata2 = false
                            BringKata3 = false
                            BringKata4 = true
                            FastAttackBoss = false
                      for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                      if v.Name == "Baking Staff [Lv. 2250]" then
                          repeat game:GetService("RunService").Heartbeat:wait()
                            if v.Name == v.Name then
                                spawn(function()
                                BringMob()
                                end)
                                spawn(function()
                                PosMon4 = v.HumanoidRootPart.CFrame  
                                end)
                                    EquipWeapon(set.KatakuriPickedWeapons)
                                    TP(v.HumanoidRootPart.CFrame * CFrame.new(0,set.Y,0))         
                                    Attack(task.wait(0.05))  
