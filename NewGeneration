                                    TP(v.HumanoidRootPart.CFrame * CFrame.new(0,set.Y,0))         
                                    Attack(task.wait(0.05))  
                              spawn(function()
                                wait(3)
                                 if v.Humanoid.Health == v.Humanoid.MaxHealth then
             v.Humanoid.Health = 0
                                 end
                             end)
                            end
                          until v.Humanoid.Health <= 0 or not v.Parent or not AutoFarmBones
                      end
                      end
                    elseif c.Name == "Living Zombie [Lv. 2000]" and c.Humanoid.Health > 0 then
                        BringBone1 = false
                        BringBone2 = true
                        BringBone3 = false
