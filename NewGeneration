                 table.insert(LawWeapons ,v.Name)
              end
          end
          for i,v in pairs(game:GetService('Players').LocalPlayer.Character:GetChildren()) do  
              if v:IsA("Tool") then
                 table.insert(LawWeapons, v.Name)
              end
          end
        end)
      
        if set.LawPickedWeapons ~= nil then
          WeaponSection2:UpdateDrop(set.LawPickedWeapons)
      end
end
