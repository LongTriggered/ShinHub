local drop = Section:NewDropdown("Auto Join:", "DropdownInf", {"Pirate", "Marine"}, function(currentOption)

    if currentOption == "Pirate" then
        set.Pirate = true
        set.Marine = false
    elseif currentOption == "Marine" then
        set.Pirate = false
        set.Marine = true
    end
end)

if set.Pirate == true then
    drop:UpdateDrop("Auto Join: Pirate")
elseif set.Marine == true then
    drop:UpdateDrop("Auto Join: Marine")
end

