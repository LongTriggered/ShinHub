    Section:NewToggle("Auto Buy Raid Law Chip", "ToggleInfo",AutoBuyChipLaw, function(state)
       AutoBuyChipLaw = state
    end)
    WeaponSection2 = Section:NewDropdown("Select Weapons","", LawWeapons, function(Value)
        set.LawPickedWeapons = Value
        end)
        Section:NewButton("Refresh Weapons", "Refreshes Dropdown", function()
          for g in pairs (LawWeapons) do
